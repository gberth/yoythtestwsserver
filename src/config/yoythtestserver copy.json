{
    "stream": {
        "streamId": "yoythtestserver",
        "streamClass": "AController",
        "streamConfig": {
            "log": 30,
            "streams": [
                {
                    "streamId": "openbankingrest",
                    "streamClass": "RestServer",
                    "streamConfig": {
                        "name": "banking",
                        "version": "1.0.0",
                        "port": "REST_PORT",
                        "baseroute": "",
                        "raw": true,
                        "autoStart": true,
                        "routes": [
                            {
                                "action": "get",
                                "route": "/yts/apiadmin/ping",
                                "type": "bank.ping"
                            },
                            {
                                "route": "/yts/bankdata/:data",
                                "type": "proxy.bank"
                            }
                        ],
                        "timeout": 2000
                    }
                },
                {
                    "streamId": "wsserver",
                    "streamClass": "WsServer",
                    "streamConfig": {
                        "raw": true,
                        "payloadonly": true,
                        "autoStart": true,
                        "port": "WEBSOCKET_PORT",
                        "raw_type": "proxy.ws"
                    }
                },
                {
                    "streamId": "proxyserver",
                    "streamClass": "ProxyServer",
                    "streamConfig": {
                        "autoStart": true,
                        "return_type": "proxyreturn"
                    }
                }
            ],
            "dispatcherMessages": [
                {
                    "type": "proxy.*",
                    "streams": [
                        {
                            "streamId": "proxyserver"
                        }
                    ]
                },
                {
                    "type": "proxyreturn",
                    "streams": [
                        {
                            "streamId": "wsserver"
                        }
                    ]
                }
            ],
            "defaults": {}
        }
    }
}